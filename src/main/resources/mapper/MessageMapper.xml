<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="vip.panzhigao.mapper.MessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="vip.panzhigao.entity.Message">
        <id column="id" property="id" />
        <result column="status" property="status" />
        <result column="receiver_user_id" property="receiverUserId" />
        <result column="sender_name" property="senderName" />
        <result column="sender_user_id" property="senderUserId" />
        <result column="message_type" property="messageType" />
        <result column="article_id" property="articleId" />
        <result column="comment_id" property="commentId" />
        <result column="content_name" property="contentName" />
        <result column="create_time" property="createTime" />
        <result column="comment_content" property="commentContent" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, status, receiver_user_id, sender_name, sender_user_id, message_type, article_id, comment_id, content_name, create_time, comment_content
    </sql>

</mapper>
